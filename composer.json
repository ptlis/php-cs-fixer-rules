{
    "name": "ptlis/php-cs-fixer-rules",
    "description": "Custom rules for php-cs-fixer",
    "license": "MIT",
    "keywords": [ "php-cs-fixer" ],
    "type": "library",
    "authors": [
        {
            "name": "brian ridley",
            "email":"ptlis@ptlis.net"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-pcntl": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.6",
        "phpunit/phpunit": "^9.5.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "ptlis\\PhpCsFixerRules\\": "src/",
            "ptlis\\PhpCsFixerRules\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
